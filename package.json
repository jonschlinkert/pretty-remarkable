{
  "name": "pretty-remarkable",
  "description": "Plugin for prettifying markdown with Remarkable using custom renderer rules.",
  "version": "0.2.0",
  "homepage": "https://github.com/jonschlinkert/pretty-remarkable",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/pretty-remarkable",
  "bugs": {
    "url": "https://github.com/jonschlinkert/pretty-remarkable/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib/rules.js",
    "lib/utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "lazy-cache": "^1.0.2",
    "list-item": "^1.0.1",
    "markdown-utils": "^0.7.1",
    "repeat-string": "^1.5.2"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.7",
    "mocha": "*",
    "remarkable": "^1.6.1"
  },
  "keywords": [
    "beautify",
    "format",
    "markdown",
    "marked",
    "md",
    "plugin",
    "prettify",
    "pretty",
    "remarkable"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "markdown-toc",
        "markdown-utils",
        "prettify-markdown",
        "remarkable"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}